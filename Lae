print('see')
local function MakeResizable(frame)
    local resizing = false
    local startSize, startPos

    local ResizeHandle = Instance.new("Frame")
    ResizeHandle.Size = UDim2.new(0.5, 0, 0, 2)
    ResizeHandle.Position = UDim2.new(0.5, 0, 1, -3)
    ResizeHandle.AnchorPoint = Vector2.new(0.5, 1)
    ResizeHandle.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    ResizeHandle.BorderSizePixel = 0
    ResizeHandle.Parent = frame

    local UICorner = Instance.new("UICorner")
    UICorner.CornerRadius = UDim.new(0, 1)
    UICorner.Parent = ResizeHandle

    ResizeHandle.MouseEnter:Connect(function()
        ResizeHandle.BackgroundColor3 = Color3.fromRGB(150, 150, 150)
    end)

    ResizeHandle.MouseLeave:Connect(function()
        ResizeHandle.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    end)

    ResizeHandle.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            resizing = true
            startSize = frame.Size
            startPos = Vector2.new(input.Position.X, input.Position.Y)
        end
    end)

    ResizeHandle.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            resizing = false
        end
    end)

    game:GetService("UserInputService").InputChanged:Connect(function(input)
        if resizing and input.UserInputType == Enum.UserInputType.MouseMovement then
            local deltaY = input.Position.Y - startPos.Y
            frame.Size = UDim2.new(
                startSize.X.Scale, startSize.X.Offset,
                0, math.max(300, startSize.Y.Offset + deltaY)
            )
        end
    end)
end
